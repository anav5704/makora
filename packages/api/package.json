{
    "name": "@makora/api",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./src/index.ts"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "default": "./src/*.ts"
        }
    },
    "scripts": {
        "build": "tsdown"
    },
    "devDependencies": {
        "tsdown": "catalog:tsdown"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@trpc/client": "catalog:trpc",
        "@trpc/server": "catalog:trpc",
        "next": "catalog:next",
        "dotenv": "catalog:dotenv",
        "@makora/auth": "workspace:*",
        "@makora/db": "workspace:*"
    }
}